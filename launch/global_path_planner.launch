<?xml version="1.0"?>
<launch>
    <!-- yaml file path -->
    <arg name="global_path_planner_settings" default="$(find gpp_tester)/map/map3.yaml"/>
    <!-- node launch -->
    <node pkg="gpp_tester" type="global_path_planner" name="global_path_planner" >
        <rosparam command="load" file="$(arg global_path_planner_settings)"/>
    </node>
</launch>
